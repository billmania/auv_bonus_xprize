[DEFAULT]
logfile = /tmp/auv_bonus_xprize.log
version = 3

[starting]
; UTM positions as (Easting,Northing),
; rounded to the nearest integer.
auv_position_utm = 80,150
auv_heading_deg = 350

; contest boundaries
northwest_utm = 10,300
northeast_utm = 100,300
southeast_utm = 100,100
southwest_utm = 10,100

; set in integer true compass degrees, drift in float knots
set = 100
drift = 0.0

[variables]
easting_x = NAV_X
northing_y = NAV_Y
depth = PS_DEPTH
heading = IMU_HEADING

[auv]
host = 192.168.0.210
port = 9000
client_name = xprize

distance_tolerance = 1.0
depth_tolerance = 1.0
max_prop_speed = 10.0
max_elevator = 5.0
max_rudder = 5.0

[search]
max_depth_meters = 20.0
min_depth_meters = 3.0
boundary_buffer_meters = 5.0
track_separation_meters = 4.0
max_time_without_gps_fix_minutes = 30
max_speed_m_per_s = 0.5
max_turn_angle_radians = 0.785
sea_current_set_true_deg = 90
sea_current_drift_knots = 0.0

[navigation]
position_accuracy_meters = 3.0
meters_per_nm = 1852.0
